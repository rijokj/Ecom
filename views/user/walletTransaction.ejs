<section class="user-dashboard page-wrapper">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<ul class="list-inline dashboard-menu text-center">
					<li><a  href="/dashboard">Dashboard</a></li>
					<li><a href="/profileOrderList">Orders</a></li>
					<li><a href="/ProfileAddress">Address</a></li>
					<li><a href="/profileDetails">Profile Details</a></li>
                    <li><a class="active" href="/wallet">Transactions</a></li>

				</ul>
				<div class="dashboard-wrapper user-dashboard">
					<div class="media">
						
						<div class="table-responsive">
                            <table id="myTable2" class="table" >
                                <thead>
                                    <tr>
                                        <th>Sl No</th>
                                        <th>Date</th>
                                        <th>Amount</th>
                                        <th>Transaction type</th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% wallet.forEach(function(wallet,index) {%>
                                    <tr>
                                        <td><%=index+1%></td>
                                        <td><%=wallet.walletTransaction.date.toLocaleString()%></td>
                                        <td>₹ <%=wallet.walletTransaction.amount%></td>
                                        <%if(wallet.walletTransaction.type=="Credit"){%>
                                        <td><span class="label label-success"><%=wallet.walletTransaction.type%></span></td>
                                        <%}else{%>
                                        <td><span class="label label-danger"><%=wallet.walletTransaction.type%></span></td>
                                        <%}%>
                                    </tr>
                                    <% }) %>
  
                                </tbody>
                            </table>
                        </div>
					</div>
					<div class="total-order mt-20">
						
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
